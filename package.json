  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/jwt": "9.1.0",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "@prisma/client": "6.9.0",
    "bcryptjs": "3.0.2",
    "fastify": "5.3.3",
    "fastify-type-provider-zod": "4.0.2",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.3",
    "zod": "3.25.50"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "9.8.0",
    "@types/node": "22.15.19",
    "@types/supertest": "6.0.3",
    "dotenv": "16.5.0",
    "prisma": "6.9.0",
    "supertest": "7.1.1",
    "tsup": "8.5.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  }
